{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/test",
      "dest": "./index.js"
    },
    {
      "src": "/browse",
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ]
}
